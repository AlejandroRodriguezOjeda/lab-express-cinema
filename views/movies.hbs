<h1>{{title}}</h1>

<p>click on the movies to see the showtime!</p>
<div class="movies-container">

  {{#each this.allMovies}}
  <div class="movie-card">
    <h2>{{this.title}}</h2>
    <img src="{{image}}" alt="{{title}}">
     <a href="/movies/{{this.id}}">Show more</a>
    </div>
 
  {{/each}}

</div>